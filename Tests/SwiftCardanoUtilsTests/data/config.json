{
    "cardano": {
        "cli": "cardano-cli",
        "node": "cardano-node",
        "hw_cli": "cardano-hw-cli",
        "signer": "cardano-signer",
        "socket": "node.socket",
        "config": "config.json",
        "topology": "topology.json",
        "database": "db",
        "immutable_database": "db/immutable",
        "volatile_database": "db/volatile",
        "port": 3001,
        "host_addr": "0.0.0.0",
        "host_ipv6_addr": "::1",
        "validate_db": false,
        "non_producing_node": false,
        "tracer_socket_path_accept": "./tracer-accept.socket",
        "tracer_socket_path_connect": "./tracer-connect.socket",
        "byron_delegation_certificate": "byron-delegation.cert",
        "byron_signing_key": "byron-signing.key",
        "shelley_kes_key": "shelley-kes.skey",
        "shelley_vrf_key": "shelley-vrf.skey",
        "shelley_operational_certificate": "shelley-operational.cert",
        "bulk_credentials_file": "bulk-credentials.json",
        "shutdown_ipc": 42,
        "shutdown_on_slot_synced": 1000000,
        "shutdown_on_block_synced": "block123abc",
        "mempool_capacity_override": 2048,
        "no_mempool_capacity_override": false,
        "network": "mainnet",
        "era": "conway",
        "ttl_buffer": 100000,
        "working_dir": "./",
        "show_output": true
    },
    "ogmios": {
        "binary": "ogmios",
        "host": "0.0.0.0",
        "port": 1337,
        "timeout": 100,
        "max_in_flight": 50,
        "log_level": "debug",
        "log_level_health": "debug",
        "log_level_metrics": "debug",
        "log_level_websocket": "debug",
        "log_level_server": "debug",
        "log_level_options": "debug",
        "working_dir": "./ogmios",
        "show_output": true
    },
    "kupo": {
        "binary": "kupo",
        "host": "0.0.0.0",
        "port": 1442,
        "since": "origin",
        "matches": [],
        "defer_db_indexes": true,
        "prune_utxo": false,
        "gc_interval": 300,
        "max_concurrency": 10,
        "in_memory": false,
        "log_level": "info",
        "log_level_http_server": "info",
        "log_level_database": "info",
        "log_level_consumer": "info",
        "log_level_garbage_collector": "info",
        "log_level_configuration": "info",
        "working_dir": "kupo.db",
        "show_output": true
    }
}
